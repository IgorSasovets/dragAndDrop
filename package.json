{
  "name": "protractor-firefox-support",
  "version": "1.3.4",
  "description": "This lib contains implementation of basic actions which can be used for e2e testing in Firefox browser",
  "main": "support.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "install-modules-tests": "cd tests && npm install",
    "webdriver-update-windows": "node tests\\node_modules\\protractor\\bin\\webdriver-manager update",
    "webdriver-start-windows": "node tests\\node_modules\\protractor\\bin\\webdriver-manager start",
    "test-windows": "node .\\tests\\node_modules\\protractor\\bin\\protractor tests\\config.js",
    "webdriver-update-linux": "node tests/node_modules/protractor/bin/webdriver-manager update",
    "webdriver-start-linux": "node tests/node_modules/protractor/bin/webdriver-manager start",
    "test-linux": "node ./tests/node_modules/protractor/bin/protractor config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorSasovets/protractor-firefox-support.git"
  },
  "keywords": [
    "protractor",
    "firefox",
    "actions"
  ],
  "author": "Igor Sasovets",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IgorSasovets/protractor-firefox-support/issues"
  },
  "homepage": "https://github.com/IgorSasovets/protractor-firefox-support#readme"
}
